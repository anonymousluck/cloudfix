{
  "Requests": [
    {
      "id": "allow_fe4c9225",
      "Effect": "allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::<my-bucket>",
        "arn:aws:s3:::<my-bucket>/*/"
      ],
      "Condition": {
        "StringEquals": {
          "s3:ExistingObjectTag/Department": "Finance"
        }
      }
    },
    {
      "id": "allow_4d6eadb3",
      "Effect": "allow",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::<my-bucket>/*/",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "data/partner_2/*"
          ]
        }
      }
    },
    {
      "id": "allow_b6045ae2",
      "Effect": "allow",
      "Action": [
        "s3:ListBucket",
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::<my-bucket>",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "data/partner_2/data/incoming/*"
          ]
        }
      }
    },
    {
      "id": "deny_773fbbd8",
      "Effect": "deny",
      "Action": "lambda:InvokeFunction",
      "Resource": "arn:aws:s3:::<my-bucket>/*/",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "data/partner_2/*"
          ]
        }
      }
    },
    {
      "id": "deny_b6b7c8b4",
      "Effect": "deny",
      "Action": "dynamodb:PutItem",
      "Resource": "arn:aws:s3:::<my-bucket>",
      "Condition": {
        "IpAddress": {
          "aws:SourceIp": "192.168.1.0/24"
        }
      }
    }
  ]
}