{
  "Version": "2012-10-17",
  "Id": "CrossAccountAccess",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Service": "delivery.logs.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "${bucket arn}/*",
      "Condition": {
        "StringEquals": {
          "AWS:SourceOrgID": "o-XXXXX"
        }
      },
      "Sid": "statement1"
    },
    {
      "Effect": "Allow",
      "Principal": {
        "Service": "delivery.logs.amazonaws.com"
      },
      "Action": "s3:GetBucketAcl",
      "Resource": "${bucket arn}",
      "Condition": {
        "StringEquals": {
          "AWS:SourceOrgID": "o-XXXXX"
        }
      },
      "Sid": "statement2"
    }
  ]
}