{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "imagebuilder:GetComponent",
        "imagebuilder:ListComponents",
        "imagebuilder:GetComponentPolicy"
      ],
      "Resource": "arn:aws:imagebuilder:us-east-1::component///1",
      "Sid": "statement1"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:Get*",
        "s3:List*"
      ],
      "Resource": "arn:aws:s3:::-useast",
      "Sid": "statement2"
    }
  ]
}